{
    "info": {
        "_postman_id": "code-query-collection",
        "name": "Code-Query",
        "description": "API Collection for Code-Query RAG Application",
        "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
    },
    "variable": [
        {
            "key": "base_url",
            "value": "http://127.0.0.1:8000",
            "type": "string"
        },
        {
            "key": "access_token",
            "value": "",
            "type": "string"
        },
        {
            "key": "repo_id",
            "value": "",
            "type": "string"
        }
    ],
    "auth": {
        "type": "bearer",
        "bearer": [
            {
                "key": "token",
                "value": "{{access_token}}",
                "type": "string"
            }
        ]
    },
    "item": [
        {
            "name": "Auth",
            "item": [
                {
                    "name": "Signup",
                    "request": {
                        "method": "POST",
                        "header": [],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n    \"email\": \"test@example.com\",\n    \"password\": \"password123\"\n}",
                            "options": {
                                "raw": {
                                    "language": "json"
                                }
                            }
                        },
                        "url": {
                            "raw": "{{base_url}}/auth/signup",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "auth",
                                "signup"
                            ]
                        }
                    },
                    "response": []
                },
                {
                    "name": "Login",
                    "event": [
                        {
                            "listen": "test",
                            "script": {
                                "exec": [
                                    "var jsonData = pm.response.json();",
                                    "pm.collectionVariables.set(\"access_token\", jsonData.access_token);"
                                ],
                                "type": "text/javascript"
                            }
                        }
                    ],
                    "request": {
                        "auth": {
                            "type": "noauth"
                        },
                        "method": "POST",
                        "header": [],
                        "body": {
                            "mode": "urlencoded",
                            "urlencoded": [
                                {
                                    "key": "username",
                                    "value": "test@example.com",
                                    "type": "text"
                                },
                                {
                                    "key": "password",
                                    "value": "password123",
                                    "type": "text"
                                }
                            ]
                        },
                        "url": {
                            "raw": "{{base_url}}/auth/login",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "auth",
                                "login"
                            ]
                        }
                    },
                    "response": []
                }
            ]
        },
        {
            "name": "Repositories",
            "item": [
                {
                    "name": "Ingest Repo",
                    "event": [
                        {
                            "listen": "test",
                            "script": {
                                "exec": [
                                    "var jsonData = pm.response.json();",
                                    "pm.collectionVariables.set(\"repo_id\", jsonData.id);"
                                ],
                                "type": "text/javascript"
                            }
                        }
                    ],
                    "request": {
                        "method": "POST",
                        "header": [],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n    \"github_url\": \"https://github.com/fastapi/fastapi\"\n}",
                            "options": {
                                "raw": {
                                    "language": "json"
                                }
                            }
                        },
                        "url": {
                            "raw": "{{base_url}}/repos/ingest",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "repos",
                                "ingest"
                            ]
                        }
                    },
                    "response": []
                },
                {
                    "name": "List Repos",
                    "request": {
                        "method": "GET",
                        "header": [],
                        "url": {
                            "raw": "{{base_url}}/repos/",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "repos",
                                ""
                            ]
                        }
                    },
                    "response": []
                },
                {
                    "name": "Chat",
                    "request": {
                        "method": "POST",
                        "header": [],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n    \"question\": \"How does dependency injection work?\"\n}",
                            "options": {
                                "raw": {
                                    "language": "json"
                                }
                            }
                        },
                        "url": {
                            "raw": "{{base_url}}/repos/{{repo_id}}/chat",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "repos",
                                "{{repo_id}}",
                                "chat"
                            ]
                        }
                    },
                    "response": []
                }
            ]
        }
    ]
}